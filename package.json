{
  "name": "code-risk-radar",
  "version": "1.0.0",
  "description": "AI-powered code risk analysis for pull requests with Atlassian Forge and Rovo",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm install && npm run build",
    "deploy": "forge deploy",
    "deploy:production": "npm run build && forge deploy --environment production",
    "install:app": "forge install",
    "tunnel": "forge tunnel",
    "logs": "forge logs",
    "lint": "eslint src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@forge/api": "^2.0.0",
    "@forge/bridge": "^3.0.0",
    "@forge/resolver": "^1.5.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "forge",
    "atlassian",
    "rovo",
    "security",
    "code-analysis",
    "risk-assessment",
    "pull-request"
  ],
  "author": "Your Name",
  "license": "MIT"
}
